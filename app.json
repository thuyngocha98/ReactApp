{
  "expo": {
    "name": "WeGo",
    "slug": "we-go",
    "privacy": "public",
    "androidStatusBar": {
      "backgroundColor": "#f7bd42"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "version": "3.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/wego-icon.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/wego-adap.png",
        "backgroundColor": "#FFF"
      },
      "package": "com.uit.wego",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDpNYS85EVJduxPAuABAGVpA95zT6laOq8"
        }
      },
      "useNextNotificationsApi": true,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "googleServicesFile": "./google-services.json",
      "versionCode": 3
    }
  }
}
